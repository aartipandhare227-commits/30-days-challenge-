def findleaders(arr):
    n=len(arr)
    leadera=[]


    max_from_right=arr[-1]
    ladders. append(max_from_right)

    for i in range(n-2, -1,-1):
        if arr[i] > max_from_right:
        ladders. append(arr[i])
        max_from_right = arr[i]
    
    ladders. reverse()
    return ladders 

arr=[16,17,4,3,5,1]
print("leaders:", findleaders(arr))